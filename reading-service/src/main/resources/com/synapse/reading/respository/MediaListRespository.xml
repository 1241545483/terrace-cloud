<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.synapse.reading.respository.MediaListRespository">

    <update id="updatePlayMediaNum" parameterType="map">
        update  `media_list` SET
            `play_num`=play_num+1
        where
            `rec_id` = #{recId}
    </update>

</mapper>
