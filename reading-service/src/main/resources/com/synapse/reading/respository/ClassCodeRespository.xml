<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.synapse.reading.respository.ClassCodeRespository">
    <insert id="insertAndGet" keyProperty="classCode" useGeneratedKeys="true">
        insert into `class_code`
        (`status`,`create_id`,`create_time`,`update_id`,`update_time`)
        values (#{status},#{createId},#{createTime},#{updateId},#{updateTime})
    </insert>
</mapper>
